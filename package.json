{
  "name": "react-skeleton",
  "version": "0.1.0",
  "description": "An skeleton proposal to react applications.",
  "main": "src/index.js",
  "private": true,
  "dependencies": {
    "material-ui": "^0.17.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "react-tap-event-plugin": "^2.0.1",
    "sanitize.css": "^5.0.0"
  },
  "devDependencies": {
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "react-scripts": "^0.9.4"
  },
  "scripts": {
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass public/ -o public/ --include-path public",
    "watch-css": "npm run build-css && node-sass public/ -o public/ --include-path public --watch --recursive",
    "start-js": "react-scripts start",
    "start": "NODE_PATH=src npm-run-all -p watch-css start-js",
    "build": "NODE_PATH=src npm run build-css && react-scripts build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Danilo-Araujo-Silva/ReactSkeleton.git"
  },
  "keywords": [
    "react",
    "skeleton"
  ],
  "author": "Danilo Ara√∫jo Silva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Danilo-Araujo-Silva/ReactSkeleton/issues"
  },
  "homepage": "https://github.com/Danilo-Araujo-Silva/ReactSkeleton#readme"
}
