{
  "name": "react-skeleton",
  "version": "0.1.0",
  "description": "An skeleton proposal to react applications.",
  "main": "src/index.js",
  "private": true,
  "dependencies": {
    "antd": "^2.8.0",
    "bootstrap": "4.0.0-alpha.6",
    "inferno": "^1.3.1",
    "jquery": "^3.1.1",
    "material-components-web": "^0.6.0",
    "material-design-lite": "^1.3.0",
    "material-ui": "^0.17.0",
    "materialize-css": "^0.98.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "react-tap-event-plugin": "^2.0.1",
    "react-toolbox": "^2.0.0-beta.7",
    "react-toolbox-themr": "^1.0.2",
    "sanitize.css": "^5.0.0",
    "semantic-ui": "^2.2.9",
    "vuetify": "^0.8.10"
  },
  "devDependencies": {
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "react-scripts": "^0.9.4"
  },
  "scripts": {
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass public/ -o public/ --include-path public --include-path node_modules",
    "watch-css": "npm run build-css && node-sass public/ -o public/ --include-path public --include-path node_modules --watch --recursive",
    "start-js": "react-scripts start",
    "start": "BROWSER=none NODE_PATH=src npm-run-all -p start-js",
    "build": "NODE_PATH=src npm run build-css && react-scripts build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Danilo-Araujo-Silva/ReactSkeleton.git"
  },
  "keywords": [
    "react",
    "skeleton"
  ],
  "author": "Danilo Ara√∫jo Silva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Danilo-Araujo-Silva/ReactSkeleton/issues"
  },
  "homepage": "https://github.com/Danilo-Araujo-Silva/ReactSkeleton#readme"
}
